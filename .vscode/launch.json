    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": []
        },
        {
            "name": "[DEFAULT] train_ebm.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/zatom/train_ebm.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "data.datamodule.num_workers.train=1",
                "data.datamodule.num_workers.val=1",
                "data.datamodule.num_workers.test=1",
                // "encoder.num_layers=2",
                // "ecoder.num_layers=2",
                // "trainer=ddp",
                "trainer=mps",
                "trainer.log_every_n_steps=1",
                "trainer.check_val_every_n_epoch=null",
                "trainer.val_check_interval=2",
                "+trainer.limit_val_batches=2",
            ]
        },
        {
            "name": "[OVERFIT] train_ebm.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/zatom/train_ebm.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "data.datamodule.batch_size.train=1",
                "data.datamodule.batch_size.val=1",
                "data.datamodule.batch_size.test=1",
                "data.datamodule.num_workers.train=0",
                "data.datamodule.num_workers.val=0",
                "data.datamodule.num_workers.test=0",
                "logger=wandb",
                "name='EBT-B__encoder@768_ecoder@768_joint_overfitting'",
                // "trainer=ddp",
                "trainer=mps",
                "trainer.check_val_every_n_epoch=null",
                "trainer.val_check_interval=2000",
                "trainer.overfit_batches=1"
            ]
        }
    ]
}
